    "dexie": "^3.2.5",
    "pinia": "^2.0.21",
    "quasar": "^2.8.3",
    "rxjs": "^7.8.1",
    "vue": "^3.2.38",
    "vue-query": "^1.26.0",
    "vue-router": "^4.1.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.2.1",
    "@rushstack/eslint-patch": "^1.1.4",
